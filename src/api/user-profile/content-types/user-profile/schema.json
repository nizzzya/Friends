{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "User Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "username": {
      "type": "string"
    },
    "bio": {
      "type": "blocks"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ratingAsGuest": {
      "type": "decimal"
    },
    "ratingAsHost": {
      "type": "decimal"
    },
    "location": {
      "type": "string"
    },
    "participatedEvents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "inversedBy": "participants"
    },
    "organizedEvents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "organizer"
    }
  }
}
