{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 3
    },
    "description": {
      "type": "blocks"
    },
    "dateTime": {
      "type": "datetime",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true,
      "maxLength": 64
    },
    "isPublic": {
      "type": "boolean",
      "default": true
    },
    "organizer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-profile.user-profile",
      "inversedBy": "organizedEvents"
    },
    "participants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-profile.user-profile",
      "mappedBy": "participatedEvents"
    },
    "city": {
      "type": "string",
      "required": false,
      "maxLength": 64
    },
    "tags": {
      "type": "string",
      "maxLength": 255
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "event"
    },
    "documentId": {
      "type": "string",
      "unique": true,
      "required": true
    }
  }
}
